    "\n",
    "html_content += \"\"\"\n",
    "        </ul>\n",
    "    </div>\n",
    "    \n",
    "    <div class=\"section\">\n",
    "        <p><em>Report generated automatically from evaluation results</em></p>\n",
    "    </div>\n",
    "    \n",
    "</body>\n",
    "</html>\n",
    "\"\"\"\n",
    "\n",
    "# Save HTML report\n",
    "with open(results_dir / 'evaluation_report.html', 'w') as f:\n",
    "    f.write(html_content)\n",
    "\n",
    "print(\"HTML report generated and saved!\")"
   ]
  },
